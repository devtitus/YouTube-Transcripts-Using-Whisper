# Docker Environment Configuration
# Copy this to .env and update the values

# Groq Cloud API Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_WHISPER_MODEL=whisper-large-v3-turbo
GROQ_BASE_URL=https://api.groq.com/openai/v1
GROQ_AUDIO_CODEC=aac
GROQ_AUDIO_BITRATE_KBPS=32
GROQ_CHUNK_SECONDS=600
GROQ_MAX_REQUEST_MB=15
GROQ_TIMEOUT_MS=1800000

# Local ASR Service Configuration
LOCAL_ASR_MODEL=base.en
LOCAL_CHUNK_SECONDS=600
LOCAL_MAX_FILE_MB=100
LOCAL_TIMEOUT_MS=1800000
DEFAULT_MODEL_TYPE=auto

# Service Configuration
MAX_SYNC_SECONDS=900

# Note: These are set automatically in docker-compose.yml
